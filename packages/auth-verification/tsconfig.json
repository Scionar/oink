{
  "extends": "tsconfig/database.json",
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules", "./dist"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  }
}
