{
  "name": "oink",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:up": "turbo run docker:up --filter=api",
    "test": "turbo test"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.9.0",
  "dependencies": {
    "turbo": "^1.10.7"
  }
}
